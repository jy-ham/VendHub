{
  "name": "vendhub",
  "version": "1.0.0",
  "main": "dist/backend/index.js",
  "type": "module",
  "scripts": {
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "vitest --config vitest.backend.config.ts",
    "test:frontend": "vitest --config vitest.frontend.config.ts",
    "dev:frontend": "vite --config frontend/vite.config.ts",
    "dev:backend": "tsx watch backend/index.ts",
    "build:backend": "tsc -p backend/tsconfig.json && tsc-alias -p backend/tsconfig.json",
    "dev": "concurrently -n FRONTEND,BACKEND -c bgBlue.bold,bgGreen.bold \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "npm run build:backend && vite build",
    "build:frontend": "vite build --config frontend/vite.config.ts",
    "preview": "vite preview --config frontend/vite.config.ts",
    "start:frontend": "npm run build:frontend && npm run preview",
    "generate": "drizzle-kit generate",
    "push": "drizzle-kit push",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jy-ham/VendHub.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jy-ham/VendHub/issues"
  },
  "homepage": "https://github.com/jy-ham/VendHub#readme",
  "description": "",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/testing-library__react": "^10.0.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.4",
    "concurrently": "^9.1.2",
    "jsdom": "^26.1.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vitest": "^3.2.2"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hono/node-server": "^1.14.1",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@react-google-maps/api": "^2.20.6",
    "@supabase/supabase-js": "^2.49.4",
    "argon2": "^0.43.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.0",
    "drizzle-orm": "^0.42.0",
    "hono": "^4.7.11",
    "path-to-regexp": "^8.2.0",
    "postgres": "^3.4.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-places-autocomplete": "^7.3.0",
    "react-router-dom": "^7.6.0",
    "zod": "^3.24.4"
  }
}
