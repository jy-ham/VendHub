{
  "name": "vendhub",
  "version": "1.0.0",
  "main": "dist/backend/index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:frontend": "vite --config frontend/vite.config.ts",
    "dev:backend": "tsx watch backend/index.ts",
    "build:backend": "tsc -p backend/tsconfig.json && tsc-alias -p backend/tsconfig.json",
    "dev": "concurrently -n FRONTEND,BACKEND -c bgBlue.bold,bgGreen.bold \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "npm run build:backend && vite build",
    "preview": "vite preview",
    "generate": "drizzle-kit generate",
    "push": "drizzle-kit push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jy-ham/VendHub.git"
  },
